<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.win.appbarcode20180828.main_goods_receipt_Fragment">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <Spinner
            android:id="@+id/spinner_batch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:spinnerMode="dropdown"
            android:layout_marginEnd="32dp"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="151dp"
            android:layout_marginRight="32dp">

        </Spinner>

        <Spinner
            android:id="@+id/spinner_branch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:spinnerMode="dropdown"
            android:layout_marginEnd="32dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="75dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginRight="32dp">

        </Spinner>

        <TextView
            android:id="@+id/txtBatch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Batch"
            android:textSize="15dp"
            android:textStyle="bold"
            android:layout_marginStart="30dp"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="157dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="30dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Posting Date"
            android:textSize="15dp"
            android:textStyle="bold"
            android:layout_marginStart="30dp"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/textshowdate"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="30dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Branch"
            android:textSize="15dp"
            android:textStyle="bold"
            android:layout_marginStart="30dp"
            tools:layout_constraintTop_creator="1"
            android:layout_marginTop="75dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="30dp" />

        <Button
            android:id="@+id/btn_startscan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Start Scan"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="50dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/textshowdate"
            android:layout_width="136dp"
            android:layout_height="30dp"
            android:text="date"
            android:gravity="center"
            android:layout_marginStart="108dp"
            android:layout_marginEnd="94dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/spinner_batch"
            app:layout_constraintRight_toRightOf="@+id/btn_Date"
            android:layout_marginTop="76dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="74dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="@+id/spinner_branch"
            android:layout_marginLeft="108dp"
            android:layout_marginRight="94dp" />

        <Button
            android:id="@+id/btn_Date"
            android:layout_width="88dp"
            android:layout_height="48dp"
            android:text="Date"
            android:textSize="12dp"
            android:layout_marginEnd="16dp"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/textshowdate"
            tools:layout_constraintBaseline_creator="1"
            android:layout_marginRight="16dp" />

    </android.support.constraint.ConstraintLayout>

</FrameLayout>
